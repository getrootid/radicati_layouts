{% if content %}
    {% set classes = [
        "rad-two-three-grid",
        settings.layout_width
    ] %}

  {% if settings.layout_classes is not empty %}
    {% set classes = classes|merge(settings.layout_classes|split(' ')) %}
  {% endif %}

    <div {{ attributes.addClass(classes).setAttribute('id', settings.layout_id) }}>
        <div class="rad-two-three-grid__top">
            <div {{ region_attributes.one.addClass(["rad-two-three-grid__column", "rad-two-three-grid__one"]) }}>
                {{ content.one }}
            </div>
            <div {{ region_attributes.two.addClass(["rad-two-three-grid__column", "rad-two-three-grid__two"]) }}>
                {{ content.two }}
            </div>
        </div>

        <div class="rad-two-three-grid__bottom">
            <div {{ region_attributes.three.addClass(["rad-two-three-grid__column", "rad-two-three-grid__three"]) }}>
                {{ content.three }}
            </div>
            <div {{ region_attributes.four.addClass(["rad-two-three-grid__column", "rad-two-three-grid__four"]) }}>
                {{ content.four }}
            </div>
            <div {{ region_attributes.five.addClass(["rad-two-three-grid__column", "rad-two-three-grid__five"]) }}>
                {{ content.five }}
            </div>
        </div>
    </div>
{% endif %}
